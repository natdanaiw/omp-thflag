{
"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
"palette": {
    "thai_red": "#D82636",
    "thai_white": "#FFFFFF",
    "thai_blue": "#3B3B8F",
    "black": "#111111"
},
"blocks": [
    {
    "type": "prompt",
    "alignment": "left",
    "newline": true,
    "segments": [
        {
        "type": "text",
        "style": "powerline",
        "powerline_symbol": "",
        "background": "transparent",
        "foreground": "p:thai_white",
        "template": "\u250c"
    },
    {
        "type": "os",
        "style": "powerline",
        "powerline_symbol": "",
        "background": "p:thai_red",
        "foreground": "p:thai_white",
        "properties": {
            "windows": "\ue62a",
            "linux": "\uf17c",
            "ubuntu": "\uf31b",
            "macos": "\uf179"
        },
        "template": " {{.Icon}}{{if .WSL}} (WSL){{end}} "
    },
    {
        "type": "shell",
        "style": "powerline",
        "powerline_symbol": "",
        "background": "p:thai_white",
        "foreground": "p:black",
        "template": " \uf489 {{.Name}} "
    },
    {
        "type": "session",
        "style": "powerline",
        "powerline_symbol": "",
        "background": "p:thai_blue",
        "foreground": "p:thai_white",
        "template": " \uf2c0 {{.HostName}}\ue0bd{{.UserName}} "
    },
    {
        "type": "git",
        "style": "powerline",
        "powerline_symbol": "",
        "background": "p:thai_white",
        "foreground": "p:black",
        "properties": {
            "branch_icon": " \ue0a0 "
        },
        "template": "{{.HEAD}} "
        },
        {
        "type": "text",
        "style": "powerline",
        "powerline_symbol": "",
        "background": "p:thai_red",
        "foreground": "p:thai_white",
        "template": " "
        }
    ]
},
{
    "type": "prompt",
    "alignment": "right",
    "segments": [
        {
        "type": "sysinfo",
        "style": "powerline",
        "powerline_symbol": "",
        "background": "p:thai_red",
        "foreground": "p:thai_white",
        "properties": {
            "cpu_icon": "",
            "mem_icon": "",
            "template": "{{.CPU.Used}}% CPU {{.Memory.Used}}/{{.Memory.Total}} RAM"
        }
    },
    {
        "type": "executiontime",
        "style": "powerline",
        "powerline_symbol": "",
        "background": "p:thai_white",
        "foreground": "p:black",
        "properties": {
            "style": "austin",
            "threshold": 0
        },
        "template": "{{.FormattedMs}}"
        },
        {
        "type": "time",
        "style": "powerline",
        "powerline_symbol": "",
        "background": "p:thai_blue",
        "foreground": "p:thai_white",
        "properties": {
            "time_format": "02/01/06 15:04"
        },
        "template": "{{.CurrentDate | date .Format}}"
        }
    ]
},
{
    "type": "prompt",
    "alignment": "left",
    "newline": true,
    "segments": [
        {
        "type": "path",
        "style": "powerline",
        "powerline_symbol": "",
        "background": "p:thai_red",
        "foreground": "p:thai_white",
        "properties": {
            "folder_icon": "\uf07b",
            "folder_separator_icon": "»",
            "home_icon": "\ueb06",
            "style": "agnoster"
        },
        "template": "{{.Path}}"
    }
]
},
{
"type": "prompt",
"alignment": "left",
"newline": true,
"segments": [
    {
        "type": "status",
        "style": "plain",
        "foreground": "p:thai_blue",
        "foreground_templates": [
            "{{if gt .Code 0}}p:thai_red{{end}}"
        ],
        "properties": {
            "always_enabled": true
        },
        "template": "\u276f"
    }
]
}
],
"console_title_template": "{{if .Root}}[root] {{end}}{{.Shell}} in <{{.Folder}}>",
"final_space": true,
"version": 3
}
